extends layout

block content
	if commits
		table
			for single_commit in commits
				tr
					td(style="width: 75%")= single_commit.commit.message
					td(style="width: 5%") <img style="width: 100%; height: auto;" src="#{single_commit.committer.avatar_url}" />
					td(style="width: 20%")= single_commit.committer.login
	else
		p No Commits!